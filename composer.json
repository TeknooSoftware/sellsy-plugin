{
    "name": "teknoo/sellsy-plugin",
    "type": "library",
    "description": "Wordpress plugin to generate contact form with Sellsy and send it leads captured",
    "keywords": ["sellsy", "wordpress", "module", "lead"],
    "homepage": "http://teknoo.it/sellsy-plugin",
    "license": ["MIT","GPL-3.0+"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role": "Software architect, Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/sellsy-plugin/issues",
        "source": "https://github.com/TeknooSoftware/sellsy-plugin"
    },
    "require":{
        "php": ">=5.4.0",
        "unialteri/sellsy-client": ">=0.8.0-beta,<2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0,<5.0",
        "phpunit/phpunit-mock-objects": "~2.3",
        "phpunit/phpunit-selenium": "~1.4",
        "phpunit/phpunit-story": "~1.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Teknoo\\Sellsy\\Wordpress\\": ["src/Wordpress/"],
            "Teknoo\\Tests\\Sellsy\\Wordpress\\": ["tests/Wordpress/"]
        }
    },
    "archive": {
        "exclude": ["demo", "vendor", ".idea"]
